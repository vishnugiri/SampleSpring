<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="add" class="org.cubic.SampleSpring.Address">
		<property name="addressLine1" value="20411 Apple Harvest Cicle"></property>
		<property name="addressLine2" value="Apt I"></property>
		<property name="city" value="Germantown"></property>
		<property name="state" value="MD"></property>
		<property name="zipcode" value="20876"></property>
	</bean>

	<bean id="customer" class="org.cubic.SampleSpring.Customer">
		<property name="id" value="101"></property>
		<property name="firstName" value="Vishnu"></property>
		<property name="lastName" value="Giri"></property>
		<property name="address" ref="add"></property>

	</bean>
	



</beans>